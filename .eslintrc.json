{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-console":"off",
        "linebreak-style":"off",
        "no-underscore-dangle":"off",
        "func-names":"off",
        "consistent-return":"off",
        "max-classes-per-file":"off",
        "class-methods-use-this":"off",
        "import/prefer-default-export":"off"
    }
}